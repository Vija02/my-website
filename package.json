{
  "name": "my-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "prebuild": "node rss-gen.js",
    "start": "next start",
    "export": "rm -rf out && next export && touch out/.nojekyll && touch out/CNAME && echo \"michaelsalim.co.uk\" >> out/CNAME",
    "typecheck": "tsc",
    "gzip": "gzip -k ./out/_next/static/chunks/*.js && gzip -k ./out/_next/static/css/*.css",
    "gh-pages": "gh-pages -td out",
    "deploy": "yarn build && yarn export && yarn gzip && yarn gh-pages"
  },
  "dependencies": {
    "@react-hook/size": "^2.1.2",
    "@shikijs/rehype": "^1.24.0",
    "date-fns": "2.10.0",
    "feed": "^4.2.2",
    "fuzzy": "0.1.3",
    "gray-matter": "^4.0.3",
    "highlight.js": "10.2.1",
    "lerp": "1.0.3",
    "next": "13.4.1",
    "next-mdx-remote": "^5.0.0",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-autosuggest": "10.1.0",
    "react-dom": "18.2.0",
    "react-share": "^4.4.0",
    "react-three-fiber": "4.2.8",
    "react-toastify": "6.0.5",
    "react-tsparticles": "^2.0.6",
    "react-typed": "1.2.0",
    "remark": "11.0.2",
    "remark-highlight.js": "6.0.0",
    "remark-html": "10.0.0",
    "shiki": "^1.24.0",
    "three": "0.117.1",
    "threejs-meshline": "2.0.11",
    "tsparticles": "^2.0.6",
    "tsparticles-engine": "^2.0.6",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/highlightjs": "9.12.0",
    "@types/node": "20.1.2",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.2.6",
    "@types/react-autosuggest": "10.0.0",
    "@types/react-dom": "18.2.4",
    "babel-plugin-inline-react-svg": "1.1.1",
    "babel-plugin-module-resolver": "4.0.0",
    "dotenv": "8.2.0",
    "gh-pages": "3.1.0"
  }
}
