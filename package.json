{
  "name": "my-website",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "rm -rf out && next export && touch out/.nojekyll && touch out/CNAME && echo \"beta.michaelsalim.co.uk\" >> out/CNAME",
    "typecheck": "tsc",
    "gzip": "gzip -k ./out/_next/static/chunks/*.js && gzip -k ./out/_next/static/runtime/*.js && gzip -k ./out/_next/static/css/*.css",
    "gh-pages": "gh-pages -td out",
    "deploy": "yarn build && yarn export && yarn gzip && yarn gh-pages"
  },
  "dependencies": {
    "@ant-design/charts": "^0.9.4",
    "@types/nprogress": "^0.2.0",
    "@types/react-autosuggest": "^9.3.14",
    "date-fns": "2.10.0",
    "fuzzy": "^0.1.3",
    "gray-matter": "4.0.2",
    "lerp": "^1.0.3",
    "next": "latest",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "react": "^16.13.0",
    "react-autosuggest": "^10.0.2",
    "react-dom": "^16.13.0",
    "react-three-fiber": "^4.2.8",
    "react-toastify": "^6.0.5",
    "react-typed": "^1.2.0",
    "remark": "11.0.2",
    "remark-html": "10.0.0",
    "three": "^0.117.1",
    "threejs-meshline": "^2.0.11",
    "typescript": "^3.9.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "dotenv": "8.2.0",
    "gh-pages": "^3.0.0"
  }
}
